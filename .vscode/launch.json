{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Electron",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
        "windows": {
          "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
        },
        "args" : ["."],
        "env": {
            "DEVMODE": "true"
        },
        "outputCapture": "std"
      }
    ]
  }